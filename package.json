  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "concurrently": "^9.2.0",
    "cookie-parser": "^1.4.7",
    "datauri": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.1.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
